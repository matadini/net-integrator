<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
             xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="SCRIPT_PERSISTANCE">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>pl.inzynier.netintegrator.script.core.Script</class>
        <shared-cache-mode>NONE</shared-cache-mode>
    </persistence-unit>

    <persistence-unit name="MAPPING_PERSISTANCE">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>pl.inzynier.netintegrator.mapping.core.UrlMapping</class>
        <shared-cache-mode>NONE</shared-cache-mode>
    </persistence-unit>

    <persistence-unit name="USER_PERSISTANCE">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>pl.inzynier.netintegrator.user.core.User</class>
        <shared-cache-mode>NONE</shared-cache-mode>
    </persistence-unit>

</persistence>