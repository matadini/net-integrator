<fx:root prefHeight="558.0" prefWidth="744.0" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TabPane BorderPane.alignment="CENTER">
         <tabs>
            <Tab closable="false" text="      Add      ">
               <content>
                  <GridPane hgap="5.0" vgap="5.0">
                    <columnConstraints>
                      <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
                      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                    </rowConstraints>
                     <children>
                        <VBox alignment="CENTER" GridPane.columnSpan="4">
                           <children>
                              <Label text="URL Mapping">
                                 <font>
                                    <Font name="System Bold" size="14.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                        <Label text="Publish Endpoint URL" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <Label text="Publish Endpoint Method:" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                        <Label text="Target Endpoint URL:" GridPane.rowIndex="1" />
                        <Label text="Target Endpoint Method" GridPane.rowIndex="2" />
                        <Label text="Target Endpoint Server:" GridPane.rowIndex="3" />
                        <ComboBox fx:id="comboboxPublishMethod" maxWidth="1.7976931348623157E308" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                        <ComboBox fx:id="comboboxTargetMethod" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <TextField fx:id="textfieldPublishURL" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                        <TextField fx:id="textfieldTargetURL" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="textfieldTargetServer" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <HBox alignment="CENTER_RIGHT" GridPane.columnIndex="3" GridPane.rowIndex="3">
                           <children>
                            <Button fx:id="buttonAdd" mnemonicParsing="false" prefWidth="100.0" text="Add" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.columnSpan="4" GridPane.rowIndex="4">
                           <children>
                              <Label text="..." />
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </GridPane>
               </content>
            </Tab>
            <Tab closable="false" text="Edit and remove">
              <content>
                  <GridPane hgap="10.0" vgap="10.0">
                     <columnConstraints>
                        <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="250.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <TableView fx:id="tableViewUrlMapping" prefHeight="200.0" prefWidth="200.0">
                          <columns>
                            <TableColumn fx:id="tableColumnUrlMapping" prefWidth="75.0" text="Mappings" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" GridPane.columnIndex="1">
                           <children>
                              <HBox alignment="CENTER_RIGHT" spacing="10.0">
                                 <children>
                                    <Button mnemonicParsing="false" prefWidth="100.0" text="Button" />
                                    <Button mnemonicParsing="false" prefWidth="100.0" text="Button" />
                                 </children>
                              </HBox>
                              <GridPane hgap="10.0" vgap="10.0" VBox.vgrow="ALWAYS">
                                <columnConstraints>
                                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" minWidth="10.0" prefWidth="150.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                                </rowConstraints>
                                 <children>
                                    <Label text="Publish Endpoint URL" />
                                    <Label text="Publish Endpoint Method:" GridPane.rowIndex="1" />
                                    <Label text="Target Endpoint URL:" GridPane.rowIndex="2" />
                                    <Label text="Target Endpoint Method" GridPane.rowIndex="3" />
                                    <Label text="Target Endpoint Server:" GridPane.rowIndex="4" />
                                    <TextField fx:id="textfieldPublishURL1" GridPane.columnIndex="1" />
                                    <ComboBox fx:id="comboboxPublishMethod1" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                    <TextField fx:id="textfieldTargetURL1" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                    <ComboBox fx:id="comboboxTargetMethod1" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                    <TextField fx:id="textfieldTargetServer1" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                                 </children>
                              </GridPane>
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </GridPane>
              </content>
            </Tab>
         </tabs>
      </TabPane>
   </center>
</fx:root>
