<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="558.0" prefWidth="744.0" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane hgap="5.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <VBox alignment="CENTER" GridPane.columnSpan="4">
                     <children>
                        <Label text="Details">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font></Label>
                     </children>
                  </VBox>
                  <Label text="Publish Endpoint URL" GridPane.rowIndex="1" />
                  <Label text="Publish Endpoint Method:" GridPane.rowIndex="2" />
                  <Label text="Target Endpoint URL:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Label text="Target Endpoint Method" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Label text="Target Endpoint Server:" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <ComboBox fx:id="comboboxPublishMethod" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <ComboBox fx:id="comboboxTargetMethod" maxWidth="1.7976931348623157E308" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                  <TextField fx:id="textfieldPublishURL" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="textfieldTargetURL" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <TextField fx:id="textfieldTargetServer" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                  <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnSpan="2" GridPane.rowIndex="3">
                     <children>
                      <Button fx:id="buttonAdd" mnemonicParsing="false" prefWidth="75.0" text="Add" />
                        <Button fx:id="buttonSave" mnemonicParsing="false" prefWidth="75.0" text="Edit" />
                        <Button fx:id="buttonRemove" mnemonicParsing="false" prefWidth="75.0" text="Remove" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </GridPane>
            <TableView fx:id="tableViewUrlMapping" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="tableColumnUrlMapping" prefWidth="75.0" text="Mappings" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
</fx:root>
